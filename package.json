{
	"name": "code-bench",
	"version": "0.0.11",
	"description": "Stable benchmarking tool for determining code performance",
	"license": "MIT",
	"repository": "stephan-nordnes-eriksen/code-bench",
	"author": {
		"name": "Stephan Nordnes Eriksen",
		"email": "stephanruler@gmail.com",
		"url": "https://snestudio.com"
	},
	"engines": {
		"node": ">=12"
	},
	"scripts": {
		"build": "tsc",
		"prepack": "npm run build",
		"debug": "npm run build && npm run watch-debug",
		"lint": "tsc --noEmit && eslint \"src/**/*.{js,ts}\"",
		"lint-fix": "tsc --noEmit && eslint \"src/**/*.{js,ts}\" --fix",
		"test": "jest --forceExit --coverage --verbose",
		"watch-test": "npm run test -- --watchAll",
		"watch-ts": "tsc -w",
		"patch-release": "standard-version && git push --follow-tags && npm publish"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist/"
	],
	"keywords": [
		"code-bench",
		"benchmark",
		"speed",
		"profiling"
	],
	"dependencies": {},
	"devDependencies": {
		"@types/jest": "^27.0.2",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/parser": "^4.33.0",
		"ava": "^3.15.0",
		"eslint": "^7.32.0",
		"jest": "^27.2.5",
		"standard-version": "^9.3.1",
		"ts-jest": "^27.0.5",
		"typescript": "^4.4.3",
		"xo": "^0.37.0"
	}
}
